<%= stylesheet_link_tag "leaderboard" %>
<h1>Leaderboard</h1>
                <table class="table table-striped" >
		 <thead>
		    <tr>
                        <th>
                            Name
                        </th>
                        <th>
                            Score
                        </th>
                    </tr>
		</thead>
		<tbody>
		    <% @all.each do |i|%>
			<tr>
				<td><%= i.name%></td>
				<td><%= i.score%></td>
			</tr>			
		    <% end %>
		</tbody>
                </table>
            
